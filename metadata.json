{
  "name": "puppetfinland-sudo",
  "version": "0.5.1",
  "author": "Puppet-Finland team",
  "summary": "Setup sudo and configure sudo directives",
  "license": "BSD-2-Clause",
  "source": "https://github.com/Puppet-Finland/puppet-sudo.git",
